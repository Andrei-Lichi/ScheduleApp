<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ClassListViewController">
    <children>
        <Button layoutX="472.0" fx:id="addButton" layoutY="73.0" mnemonicParsing="false" onAction="#onAddButtonClick" text="Add" />
        <Button layoutX="460.0" fx:id="removeButton" layoutY="167.0" mnemonicParsing="false" onAction="#onRemoveButtonClick" text="Remove" />
        <Button layoutX="471.0" fx:id="editButton" layoutY="112.0" mnemonicParsing="false" onAction="#onEditButtonClick" text="Edit" />
        <Text layoutX="84.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Class List">
            <font>
                <Font size="35.0" />
            </font>
        </Text>
        <Button layoutX="470.0" layoutY="285.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back" />
        <TableView fx:id="tableView" onMouseClicked="#onChosenCell" layoutX="23.0" layoutY="54.0" prefHeight="200.0" prefWidth="415.0" />
        <Button layoutX="464.0" layoutY="216.0" mnemonicParsing="false" onAction="#onImportFileButtonClick" text="Import" />
        <TextField fx:id="textField" layoutX="434.0" layoutY="325.0" onKeyTyped="#onNewFilter" />
      <Button layoutX="451.0" fx:id="detailsButton" layoutY="34.0" mnemonicParsing="false" onAction="#onViewDetailsClick" text="View Details" />
    </children>
</AnchorPane>
