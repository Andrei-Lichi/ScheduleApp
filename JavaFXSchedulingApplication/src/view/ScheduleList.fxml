<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ScheduleListViewController">
   <children>
      <ListView fx:id="listView" onMouseClicked="#handleMouseClick" layoutX="102.0" layoutY="100.0" prefHeight="200.0" prefWidth="396.0" />
      <Text layoutX="231.0" layoutY="41.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Choose a class to proceed:" />
      <Button fx:id="confirmButton" onMouseClicked="#switchToCalendar" layoutX="498.0" layoutY="350.0" mnemonicParsing="false" text="Proceed" />
      <Button fx:id="cancelButton" onMouseClicked="#switchToMenu" layoutX="50.0" layoutY="350.0" mnemonicParsing="false" text="Cancel" />
   </children>
</AnchorPane>
